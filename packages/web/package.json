{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "npm run test -- --coverage",
    "test:badges": "npm run test:coverage && jest-coverage-badges",
    "test:mutation": "stryker run",
    "lint": "eslint ./src --ext .jsx,.js,.tsx,.ts",
    "lint:fix": "eslint ./src --ext .jsx,.js,.tsx,.ts --fix",
    "ts:check": "tsc --p ./tsconfig.json --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:smoke": "cypress run --spec=cypress/integration/smoke/*"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "core": "0.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.3.0",
    "@stryker-mutator/jest-runner": "^3.3.0",
    "@stryker-mutator/typescript": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^10.2.10",
    "cypress": "^4.8.0"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn ts:check && yarn test --watchAll=false"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write"
    ],
    "*.{jsx,js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "license": "MIT"
}
