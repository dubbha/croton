{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "start:dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "build": "tsc",
    "build:serve": "tsc && node ./dist/server.js",
    "migration:generate:linux": "ts-node ./node_modules/.bin/typeorm migration:generate -n migration",
    "migration:generate:windows": "ts-node ../../node_modules/typeorm/cli.js migration:generate -n migration",
    "migration:run:linux": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:run:windows": "ts-node ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert:linux": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migration:revert:windows": "ts-node ../../node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/passport": "^1.0.3",
    "@types/passport-facebook-token": "^0.4.34",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^6.6.0",
    "jest": "^26.0.1",
    "ts-class-autobind": "^0.3.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49"
  }
}
