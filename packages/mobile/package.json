{
  "name": "mobile",
  "version": "0.0.1",
  "scripts": {
    "compile": "tsc -b --incremental",
    "compile:watch": "yarn run compile -w",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "dev": "run-p compile:watch start",
    "start:android": "run-p dev android",
    "start:ios": "run-p dev ios",
    "studio": "studio android",
    "xcode": "open ios/myprojectname.xcworkspace",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.7.0",
    "core": "0.0.1",
    "react": "16.13.1",
    "react-devtools": "^4.8.1",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.9.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "0.0.7",
    "@types/react": "16.9.19",
    "@types/react-native": "0.61.12",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "24.9.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "jest": "24.9.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "16.13.1",
    "typescript": "3.7.5"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": ["node_modules/(?!(jest-)?react-native|@?react-navigation)"],
    "setupFiles": ["../../node_modules/react-native-gesture-handler/jestSetup.js"]
  },
  "license": "MIT"
}
