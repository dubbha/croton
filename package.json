{
  "name": "croton",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "cpd": "jscpd ./packages/*/src",
    "cpd:ci": "yarn cpd --silent --threshold 20",
    "server:update": "cd packages/server && yarn server:update",
    "server:deploy": "cd packages/server && yarn server:deploy"
  },
  "dependencies": {
    "react-native": "0.61.5"
  },
  "license": "MIT",
  "devDependencies": {
    "jscpd": "^3.3.0-rc.9"
  }
}
